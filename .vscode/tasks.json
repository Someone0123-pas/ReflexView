{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build whole project",
            "command": "/usr/bin/env",
            "args": [
                "make",
                "debug",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
        }
    ],
    "version": "2.0.0"
}